{
  "name": "app-webweaver",
  "version": "0.3.1",
  "description": "Devebot express-based webweaver layerware",
  "homepage": "https://github.com/apporo/app-webweaver.git",
  "author": {
    "name": "apporo",
    "email": "apporo@devebot.com",
    "url": "https://github.com/apporo"
  },
  "main": "index.js",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "bin/",
    "config/",
    "data/",
    "lib/"
  ],
  "scripts": {
    "build": "babel ./src -d ./lib",
    "clean": "rm -rf ./lib",
    "prepublish": "npm run clean && cross-env NODE_ENV=${DEVEBOT_NODE_ENV:-production} npm run build",
    "pretest": "npm run clean && cross-env NODE_ENV=${DEVEBOT_NODE_ENV:-test} npm run build",
    "test": "mocha ./test/{bdd,tdd}/*test.js",
    "verify": "npm run prepublish && mocha ./test/{bdd,tdd}/*test.js",
    "eslint": "eslint ./src/ --quiet"
  },
  "keywords": [
    "devebot",
    "devebot-plugin"
  ],
  "peerDependencies": {
    "app-webserver": "^0.3.0",
    "devebot": "^0.3.0"
  },
  "dependencies": {
    "app-webserver": "^0.3.0",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "connect-mongo": "2.0.3",
    "connect-redis": "3.4.1",
    "cookie-parser": "1.4.4",
    "cors": "2.8.4",
    "csurf": "1.10.0",
    "devebot": "^0.3.0",
    "ejs": "2.5.7",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "helmet": "3.9.0",
    "method-override": "2.3.10",
    "session-file-store": "1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-minify-mangle-names": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.1.4"
  }
}
